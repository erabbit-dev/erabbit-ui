{
  "name": "erabbit",
  "version": "0.0.1",
  "description": "Erabbit UI Components built with Vue3",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write packages/",
    "test": "pnpm --dir packages/erabbit test",
    "coverage": "pnpm --dir packages/erabbit coverage",
    "build": "pnpm --dir packages/erabbit build",
    "release": "pnpm --dir packages/erabbit release",
    "dev": "pnpm --dir packages/erabbit dev",
    "build-site": "pnpm --dir packages/docs docs:build",
    "dev-site": "pnpm --dir packages/docs docs:dev",
    "build-cli": "pnpm --dir packages/erabbit-cli build",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.js -e -V"
  },
  "lint-staged": {
    "*.{md}": [
      "prettier --write"
    ],
    "*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "zhoushugang",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@erabbit-dev/cli": "workspace:^1.0.0",
    "@erabbit-dev/eslint-config": "workspace:^1.0.0",
    "@erabbit-dev/icons": "workspace:^1.0.0",
    "@erabbit-dev/auto-import": "workspace:*",
    "@tsconfig/node18": "^18.2.1",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/coverage-v8": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.4.1",
    "erabbit": "workspace:*",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "sass": "^1.66.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.34.3",
    "vitest-fetch-mock": "^0.2.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.13"
  },
  "engines": {
    "node": ">=16.19.0",
    "pnpm": "8.7.6"
  },
  "packageManager": "pnpm@8.7.6"
}
